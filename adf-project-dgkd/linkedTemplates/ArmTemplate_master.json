{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-project-dgkd"
		},
		"adf_to_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adf_to_AzureDataLakeStorage'"
		},
		"onpremisesqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'onpremisesqlserver'",
			"defaultValue": "Integrated Security=False;Data Source=DESKTOP-E4688IK\\SQLSERVER;Initial Catalog=AdventureWorksLT2019;User ID=dgkd"
		},
		"AzureKeyVault_adf_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-project-dgkd.vault.azure.net/"
		},
		"adf_to_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2projectdgkd.dfs.core.windows.net/"
		},
		"AzureDatabricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0302-231624-vvtaavlh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-project-dgkd_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adf_to_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('adf_to_AzureDataLakeStorage_accountKey')]"
					},
					"onpremisesqlserver_connectionString": {
						"value": "[parameters('onpremisesqlserver_connectionString')]"
					},
					"AzureKeyVault_adf_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_adf_properties_typeProperties_baseUrl')]"
					},
					"adf_to_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('adf_to_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"AzureDatabricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}